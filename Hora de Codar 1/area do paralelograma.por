programa {
  funcao inicio() {
    // area do paralelograma

    real base
    real altura
    real area

    escreva("Qual é o valor da base? ")
    leia(base)
    escreva("Qual é o valor da altura? ")
    leia(altura)

    area = base * altura

    escreva("O valor da área do paralelograma é ", area)

    
  }
}
