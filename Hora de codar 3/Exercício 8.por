programa {
  funcao inicio() {
    inteiro valor, i

    escreva("Digite um valor: ")
    leia(valor)

    se(valor > 0){
      i = 1

      enquanto(i <= valor){
    
      escreva(i, "\n")
      i = i + 1
      }

      }
      
      senao{
        escreva("Digite um valor maior que 0. \n")
      
      }

    
      
    }

  }

